{
  "name": "bagelpay",
  "version": "1.0.5",
  "description": "TypeScript SDK for BagelPay API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "examples:basic": "ts-node examples/basicUsage.ts",
    "examples:products": "ts-node examples/productManagement.ts",
    "examples:checkout": "ts-node examples/checkoutPayments.ts",
    "examples:subscriptions": "ts-node examples/subscriptionCustomerManagement.ts"
  },
  "keywords": [
    "bagelpay",
    "payment",
    "api",
    "sdk",
    "typescript",
    "checkout",
    "subscription",
    "ecommerce"
  ],
  "author": "BagelPay Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bagelpay/bagelpay-sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/bagelpay/bagelpay-sdk-typescript/issues"
  },
  "homepage": "https://github.com/bagelpay/bagelpay-sdk-typescript",
  "dependencies": {
    "axios": "^1.6.0",
    "bagelpay": "1.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "^8.0.0",
    "express": "^4.18.2",
    "jest": "^29.0.0",
    "ngrok": "^4.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
